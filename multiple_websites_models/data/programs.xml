<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template name="Programs" priority="21" id="programs_page_template">
            <t t-call="website.layout">
                <t t-set="pageName" t-value="'Programs'"/>
                <div id="wrap" class="oe_structure"/>
            </t>
        </template>

        <record id="programs_page" model="website.page">
            <field name="is_published">True</field>
            <field name="url">/programs</field>
            <field name="view_id" ref="multiple_websites_models.programs_page_template"/>
            <field name="website_indexed" eval="False"/>
        </record>

    </data>
</odoo>