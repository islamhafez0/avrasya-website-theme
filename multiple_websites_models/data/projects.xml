<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <template name="Projects" priority="21" id="projects_page_template">
      <t t-call="website.layout">
        <t t-set="pageName" t-value="'Projects'" />
        <div id="wrap" class="oe_structure" />
      </t>
    </template>

    <record id="projects_page" model="website.page">
      <field name="is_published">True</field>
      <field name="url">/projects</field>
      <field name="view_id" ref="multiple_websites_models.projects_page_template" />
      <field name="website_indexed" eval="False" />
    </record>

  </data>
</odoo>